{
    "name": "clean/phpdoc-md",
    "description": "Parse PHP classes and writes documentation to markdown files",
    "keywords": ["phpdoc", "markdown"],
    "type": "library",
    "license": "MIT",
    "require": {
        "phpdocumentor/reflection-docblock": "2.0.4",
        "clean/view": "0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "scrutinizer/ocular": "dev-master",
        "romannowicki/mpr": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Clean\\PhpDocMd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clean\\PhpDocMd\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "bin": [
      "bin/phpdoc-md"
    ]
}
